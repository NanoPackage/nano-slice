    "nano",
    "nano-slice",
    "nano-package"
  ],
  "author": "hustcc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NanoPackage/nano-slice/issues"
  },
  "homepage": "https://github.com/NanoPackage/nano-slice#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "jest": "^21.2.1"
  },
  "jest": {
    "testRegex": "(/test)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
